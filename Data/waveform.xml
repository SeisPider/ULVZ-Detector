<?xml version="1.0" encoding="UTF-8"?>
<sod>
    <eventArm>
        <fdsnEvent>
            <originTimeRange>
                <startTime>
                    <year>2010</year>
                    <month>8</month>
                    <day>1</day>
                </startTime>
                <endTime>
                    <year>2012</year>
                    <month>1</month>
                    <day>1</day>
                </endTime>
            </originTimeRange>
            <magnitudeRange>
                <min>6</min>
            </magnitudeRange>
        </fdsnEvent>
        <originOR>
            <originAND>
                <magnitudeRange>
                    <min>6</min>
                </magnitudeRange>
                <originDepthRange>
                    <unit>KILOMETER</unit>
                    <min>100</min>
                    <max>1000</max>
                </originDepthRange>
            </originAND>
            <magnitudeRange>
                <min>6</min>
            </magnitudeRange>
        </originOR>
        <printlineEventProcess/>
    </eventArm>
    <networkArm>
        <fdsnStation/>
        <networkCode>TA</networkCode>
        <bandCode>B</bandCode>
		<printlineChannelProcess/>
		<stationBoxArea>
		  <latitudeRange>
			<min>35</min>
			<max>38</max>
		  </latitudeRange>
		  <longitudeRange>
			<min>-97</min>
			<max>-91</max>
		  </longitudeRange>
		</stationBoxArea>
    </networkArm>
    <waveformArm>
        <distanceRange>
            <unit>DEGREE</unit>
            <min>100</min>
            <max>110</max>
        </distanceRange>
        <phaseRequest>
            <model>prem</model>
            <beginPhase>ttp</beginPhase>
            <beginOffset>
                <unit>SECOND</unit>
                <value>-120</value>
            </beginOffset>
            <endPhase>ttp</endPhase>
            <endOffset>
                <unit>SECOND</unit>
                <value>6000</value>
            </endOffset>
        </phaseRequest>
        <fdsnDataSelect/>
        <someCoverage/>
        <someDataCoverage/>
        <printlineSeismogramProcess/>
        <sacWriter/>
        <rMean/>
        <rTrend/>
		<taper>
           <width>.05</width>
           <type>HANNING</type>
        </taper>
		<transferResponse>
            <lowCut>0.005</lowCut>
            <lowPass>0.01</lowPass>
            <highPass>15</highPass>
            <highCut>25</highCut>
        </transferResponse>
		<sacWriter>
            <workingDir>processedSeismograms</workingDir>
		</sacWriter>
        <legacyExecute>
            <command>echo Sod saved this file</command>
        </legacyExecute>
    </waveformArm>
</sod>
